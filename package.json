{
  "name": "killua-ui",
  "private": true,
  "version": "0.1.0",
  "description": "A lightning-fast, modern React component library inspired by Killua from Hunter x Hunter",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate:component": "node scripts/generateComponent.mjs",
    "generate:page": "node scripts/generatePage.mjs",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.32",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jest": "^29.7.0",
    "tinycolor2": "^1.6.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "~5.5.0",
    "vite": "^6.3.1"
  },
  "author": "Jeanette O'Brien",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeanetteobr/killua-ui"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "design-system",
    "typescript",
    "killua",
    "hunter-x-hunter"
  ]
}
